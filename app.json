{
  "expo": {
    "name": "Mandala Day",
    "slug": "mandala-day",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0817"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mandaladay.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification",
          "audio",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0817"
      },
      "package": "com.mandaladay.app",
      "versionCode": 8,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.USE_EXACT_ALARM"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "themeColor": "#0b0817",
      "backgroundColor": "#0b0817",
      "name": "Mandala Day",
      "shortName": "Mandala Day"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/audio/gong.mp3"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf35cc1b-aa28-4b46-a019-396e5c8dc2b5"
      }
    },
    "owner": "gsorell",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bf35cc1b-aa28-4b46-a019-396e5c8dc2b5"
    }
  }
}
